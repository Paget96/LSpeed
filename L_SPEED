[ Yes = 1 | No = 2 ]: "
read -r option
if [ "$option" -eq 1 ]; then
	busybox echo ""
	busybox echo "Rebooting device"
	busybox echo "3"
	busybox sleep $timeshort
	busybox echo "2"
	busybox sleep $timeshort
	busybox echo "1"
	busybox sleep $timeshort
  reboot
	busybox echo "1" > /proc/sys/kernel/sysrq
