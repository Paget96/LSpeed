	if [ "$option" -eq 1 ]; then
    busybox rm -rf $init/LS00RAM_Manager_MT 2>/dev/null
    busybox rm -rf $init/LS00RAM_Manager_BL 2>/dev/null
    busybox rm -rf $init/LS00RAM_Manager_GM 2>/dev/null
  elif [ "$option" -eq 2 ]; then
busybox rm -rf $init/LS00RAM_Manager_MT 2>/dev/null
busybox rm -rf $init/LS00RAM_Manager_BL 2>/dev/null
busybox rm -rf $init/LS00RAM_Manager_GM 2>/dev/null
